{% extends '::base.html.twig' %}

{% block body %}
    <p class="col-lg-2 col-lg-offset-5 col-md-2 cold-md-offset-5 classement">Classement</p>
    <div class="classementligne">
        {% for user in classement %}
            <p class="col-lg-4 col-lg-offset-4 col-md-4 col-md-offset-4">
                {{ loop.index }} - {{ user.username }} - {{ user.score }}
            </p>
        {% endfor %}
    </div>
{% endblock %}